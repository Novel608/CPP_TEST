#include <iostream>
#include <fstream>

using namespace std;

int main() {
	ofstream of("d:\\data.txt");
	of<<"Hello CPP\n";
	cout << "!!!Hello World!!!" << endl; // prints !!!Hello World!!!
	return 0;
}
